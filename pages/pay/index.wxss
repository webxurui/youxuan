.mycart {
  width: 750rpx;
  min-height: 100vh;
  background-color: #f3efef;
  color: #000;
  padding: 1.5vh 0;
}
.mycart .HaveGoods {
  width: 750rpx;
  font-size: 30rpx;
}
.mycart .HaveGoods .goods_address .address_add,
.mycart .HaveGoods .goods_address .address_userress {
  background-color: #fff;
  border-radius: 18rpx;
  width: 710rpx;
  margin: 0 20rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .goods_address .icon-jinggao-- {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background-color: #ff734c;
  border-radius: 50%;
  line-height: 50rpx;
}
.mycart .HaveGoods .goods_address .icon1 {
  text-align: center;
  width: 100rpx;
}
.mycart .HaveGoods .goods_address .icon1 text {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background-color: #ff734c;
  border-radius: 50%;
  line-height: 50rpx;
}
.mycart .HaveGoods .goods_address .icon2 {
  width: 60rpx;
}
.mycart .HaveGoods .goods_address .icon-xiangyou {
  font-size: 28rpx;
}
.mycart .HaveGoods .goods_address .icon-dingwei,
.mycart .HaveGoods .goods_address .icon-gantanhao {
  font-size: 34rpx;
  color: #fff;
}
.mycart .HaveGoods .goods_address .icon-jiahao2fill {
  font-size: 42rpx;
  color: rgba(0, 0, 0, 0.2);
}
.mycart .HaveGoods .goods_address .newaddress {
  font-size: 32rpx;
  width: 610rpx;
  font-weight: 400;
  padding-left: 10rpx;
}
.mycart .HaveGoods .address_userress .newaddress .config {
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .address_userress .newaddress .config .config_name {
  font-size: 32rpx;
  font-weight: 500;
}
.mycart .HaveGoods .address_userress .newaddress .config .config_phone {
  font-size: 24rpx;
  color: #9ea0a3;
  padding-left: 20rpx;
}
.mycart .HaveGoods .address_userress .address {
  font-size: 26rpx;
}
.mycart .HaveGoods .address_userress .icon-dingwei1 {
  font-size: 40rpx;
}
.mycart .HaveGoods .goods_list {
  border-radius: 18rpx;
  width: 710rpx;
  margin: 20rpx;
}
.mycart .HaveGoods .goods_list:nth-child(1) {
  margin-top: 0;
}
.mycart .HaveGoods .goods_list .list_item {
  border-radius: 15rpx;
  background-color: #fff;
  margin: 15rpx 0;
  padding: 0 15rpx;
  height: 280rpx;
  width: 710rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #e9ecf0;
}
.mycart .HaveGoods .goods_list .list_item:nth-child(1) {
  margin-top: 0;
}
.mycart .HaveGoods .goods_list .list_item .item_checkbox {
  width: 10%;
  height: 100%;
  line-height: 280rpx;
}
.mycart .HaveGoods .goods_list .list_item .icon-quan {
  font-size: 46rpx;
  color: #000;
}
.mycart .HaveGoods .goods_list .list_item .icon-xuanzhong {
  font-size: 46rpx;
  color: #ff734c;
}
.mycart .HaveGoods .goods_list .list_item .item_img {
  width: 35%;
  height: 100%;
  padding: 15rpx;
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .goods_list .list_item .item_img image {
  width: 90%;
  height: 240rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config {
  position: relative;
  height: 100%;
  padding-left: 20rpx;
  width: 55%;
  box-sizing: border-box;
  font-size: 28rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .config_top {
  position: absolute;
  top: 20rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_name {
  font-size: 32rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_num {
  padding-top: 20rpx;
  display: flex;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_num .item_lable {
  padding-right: 20rpx;
  font-size: 28rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_num .item_num_delete,
.mycart .HaveGoods .goods_list .list_item .item_config .item_num .icon-jia1,
.mycart .HaveGoods .goods_list .list_item .item_config .item_num .icon-jian1 {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background-color: rgba(0, 0, 0, 0.1);
  border-radius: 4rpx;
  text-align: center;
  line-height: 40rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_num input {
  width: 60rpx;
  text-align: center;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_price {
  width: 100%;
  display: flex;
  justify-content: space-between;
  position: absolute;
  bottom: 50rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_price .price_price {
  font-size: 30rpx;
  color: #ff734c;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_price .price_delete {
  padding-right: 40rpx;
  font-size: 26rpx;
  text-decoration: underline;
  color: #ccc;
}
.mycart .HaveGoods .goods_settl_cont {
  background-color: #fff;
  border-top: 1px solid #e9ecf0;
  width: 750rpx;
  position: fixed;
  bottom: 0;
}
.mycart .HaveGoods .goods_settl {
  padding: 0 0 0 15rpx;
  display: flex;
  justify-content: flex-end;
  height: 100rpx;
}
.mycart .HaveGoods .goods_settl .settl_right {
  width: 70%;
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .goods_settl .settl_right .settl_totalnum {
  width: 20%;
  text-align: center;
  color: #ccc;
  font-size: 28rpx;
}
.mycart .HaveGoods .goods_settl .settl_right .settl_totalprice {
  width: 45%;
  text-align: right;
  padding-right: 10rpx;
  color: #ff734c;
}
.mycart .HaveGoods .goods_settl .settl_right .settl_totalprice text {
  color: #000;
  padding-right: 8rpx;
}
.mycart .HaveGoods .goods_settl .settl_right .Settlement {
  width: 35%;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  background-color: #ff734c;
}
