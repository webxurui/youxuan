.mycart {
  width: 750rpx;
  min-height: 100vh;
  background-color: #f3efef;
  color: #000;
}
.mycart .NoGoods_cont {
  padding-top: -1vh;
  width: 100vw;
  height: 100vh;
  background-color: #fff;
}
.mycart .NoGoods_cont .NoGoods {
  width: 100%;
  top: 50%;
  text-align: center;
  position: absolute;
  transform: translateY(-50%);
}
.mycart .NoGoods_cont .NoGoods .cont_img {
  width: 100%;
}
.mycart .NoGoods_cont .NoGoods .cont_img image {
  width: 300rpx;
  height: 230rpx;
}
.mycart .NoGoods_cont .NoGoods .cont_P {
  padding: 40rpx 0 20rpx 0;
  font-size: 34rpx;
}
.mycart .NoGoods_cont .NoGoods .cont_navigator {
  width: 230rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  color: #fff;
  margin: 0 auto;
  background-color: #00B26A;
  border-radius: 40rpx;
}
.mycart .HaveGoods {
  width: 750rpx;
  font-size: 30rpx;
  padding-top: 1vh;
}
.mycart .HaveGoods .goods_address {
  border-bottom: 1px solid #c0c0c0;
}
.mycart .HaveGoods .goods_address .address_add,
.mycart .HaveGoods .goods_address .address_userress {
  height: 120rpx;
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .goods_address .iconfont {
  width: 80rpx;
  text-align: center;
}
.mycart .HaveGoods .goods_address .icon-xiangyou {
  font-size: 28rpx;
}
.mycart .HaveGoods .goods_address .icon-jiahao2fill {
  font-size: 42rpx;
  color: rgba(0, 0, 0, 0.2);
}
.mycart .HaveGoods .goods_address .newaddress {
  font-size: 32rpx;
  width: 610rpx;
  font-weight: 400;
  padding-left: 10rpx;
}
.mycart .HaveGoods .address_userress .newaddress .config {
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .address_userress .newaddress .config .config_name {
  font-size: 32rpx;
  font-weight: 500;
}
.mycart .HaveGoods .address_userress .newaddress .config .config_phone {
  font-size: 28rpx;
  padding-left: 20rpx;
}
.mycart .HaveGoods .address_userress .address {
  font-size: 28rpx;
}
.mycart .HaveGoods .address_userress .icon-dingwei1 {
  font-size: 40rpx;
}
.mycart .HaveGoods .goods_list {
  border-radius: 18rpx;
  width: 710rpx;
  margin: 20rpx;
}
.mycart .HaveGoods .goods_list:nth-child(1) {
  margin-top: 0;
}
.mycart .HaveGoods .goods_list .list_item {
  border-radius: 15rpx;
  background-color: #fff;
  margin: 15rpx 0;
  padding: 0 15rpx;
  height: 280rpx;
  width: 710rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #e9ecf0;
}
.mycart .HaveGoods .goods_list .list_item:nth-child(1) {
  margin-top: 0;
}
.mycart .HaveGoods .goods_list .list_item .item_checkbox {
  width: 10%;
  height: 100%;
  line-height: 280rpx;
}
.mycart .HaveGoods .goods_list .list_item .icon-quan {
  font-size: 46rpx;
  color: #000;
}
.mycart .HaveGoods .goods_list .list_item .icon-xuanzhong {
  font-size: 46rpx;
  color: #ff734c;
}
.mycart .HaveGoods .goods_list .list_item .item_img {
  width: 35%;
  height: 100%;
  padding: 15rpx;
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .goods_list .list_item .item_img image {
  width: 90%;
  height: 240rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config {
  position: relative;
  height: 100%;
  padding-left: 20rpx;
  width: 55%;
  box-sizing: border-box;
  font-size: 28rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .config_top {
  position: absolute;
  top: 20rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_name {
  font-size: 32rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_num {
  padding-top: 20rpx;
  display: flex;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_num .item_lable {
  padding-right: 20rpx;
  font-size: 28rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_num .item_num_delete,
.mycart .HaveGoods .goods_list .list_item .item_config .item_num .icon-jia1,
.mycart .HaveGoods .goods_list .list_item .item_config .item_num .icon-jian1 {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background-color: rgba(0, 0, 0, 0.1);
  border-radius: 4rpx;
  text-align: center;
  line-height: 40rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_num input {
  width: 60rpx;
  text-align: center;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_price {
  width: 100%;
  display: flex;
  justify-content: space-between;
  position: absolute;
  bottom: 50rpx;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_price .price_price {
  font-size: 30rpx;
  color: #ff734c;
}
.mycart .HaveGoods .goods_list .list_item .item_config .item_price .price_delete {
  padding-right: 40rpx;
  font-size: 26rpx;
  text-decoration: underline;
  color: #ccc;
}
.mycart .HaveGoods .goods_settl {
  background-color: #fff;
  border-top: 1px solid #e9ecf0;
  width: 750rpx;
  height: 100rpx;
  padding: 0 0 0 15rpx;
  display: flex;
  position: fixed;
  bottom: 0;
}
.mycart .HaveGoods .goods_settl .settl_left {
  width: 225rpx;
  font-size: 32rpx;
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .goods_settl .settl_left .item_checkbox {
  display: flex;
  align-items: center;
}
.mycart .HaveGoods .goods_settl .settl_left .item_checkbox .iconfont {
  font-size: 46rpx;
  padding-right: 10rpx;
}
.mycart .HaveGoods .goods_settl .settl_left .item_checkbox .icon-quan {
  color: #000;
}
.mycart .HaveGoods .goods_settl .settl_left .item_checkbox .icon-xuanzhong {
  color: #ff734c;
}
.mycart .HaveGoods .goods_settl .settl_left .deleteALL {
  padding-left: 40rpx;
  font-size: 26rpx;
  text-decoration: underline;
  color: #ccc;
}
.mycart .HaveGoods .goods_settl .settl_right {
  width: 525rpx;
  display: flex;
}
.mycart .HaveGoods .goods_settl .settl_right .settl_total {
  width: 60%;
  line-height: 100rpx;
  text-align: right;
  padding-right: 10rpx;
  color: #ff734c;
}
.mycart .HaveGoods .goods_settl .settl_right .settl_total text {
  color: #000;
  padding-right: 8rpx;
}
.mycart .HaveGoods .goods_settl .settl_right .Settlement {
  width: 40%;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  background-color: #ff734c;
}
